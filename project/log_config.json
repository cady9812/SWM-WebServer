{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "basic": {
            "format": "%(asctime)s [%(levelname)s] %(filename)s:%(lineno)d %(name)s:%(funcName)s\n> %(message)s",
            "datefmt": "%Y년%m월%d일%H시%M분%S초"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "basic"
        },
        "file_debug": {
            "class": "logging.NullHandler",
            "level": "DEBUG",
            "formatter": "basic"
        },
        "file_error": {
            "class": "logging.NullHandler",
            "level": "ERROR",
            "formatter": "basic"
        }
    },
    "loggers": {
        "": {
            "level": "DEBUG",
            "handlers": ["console"],
            "propagate": false
        }
    }
}
